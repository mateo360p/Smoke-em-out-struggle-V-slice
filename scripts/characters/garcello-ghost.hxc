import flixel.FlxG;
import funkin.play.character.SparrowCharacter;
import funkin.play.PlayState;
import funkin.util.Constants;

class GarcelloGhostCharacter extends SparrowCharacter {
	var garcelloPurpleColor:FlxColor = 0xFF8E40A5;

	function new() {
		super('garcello-ghost');
	}

	function onCreate(event:ScriptEvent) {
		super.onCreate(event);

		PlayState.instance.healthBar.createFilledBar(garcelloPurpleColor, Constants.COLOR_HEALTH_BAR_GREEN);
	}
}
